{
  "version": 2,
  "framework": null,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "functions": {
    "packages/backend/api/index.ts": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/packages/backend/api"
    },
    {
      "source": "/(.*)",
      "destination": "/packages/frontend/$1"
    }
  ]
}
