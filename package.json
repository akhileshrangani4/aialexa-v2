{
  "name": "@aialexa/root",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.14.4",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "dotenv -e .env -- turbo run dev",
    "build": "dotenv -e .env -- turbo run build",
    "start": "dotenv -e .env -- turbo run start",
    "lint": "turbo run lint",
    "migrate": "dotenv -e .env -- turbo run migrate",
    "generate": "dotenv -e .env -- turbo run generate",
    "studio": "dotenv -e .env -- turbo run studio"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.4",
    "turbo": "^2.5.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
