  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.0",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.8",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "dotenv": "^16.4.7"
  }
}
